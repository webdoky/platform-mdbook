{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": ["error", "always"],
    "no-await-in-loop": "off",
    "no-continue": "off",
    "no-restricted-syntax": "off"
  }
}
